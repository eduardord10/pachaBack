//convierta centímetros a pulgadas y libras a kilogramos
Algoritmo sin_titulo
	Escribir "Ingrese centimetros:";
    Leer centimetros;
    Escribir Sin Saltar "Ingrese libras:";
    Leer libras;
    pulgadas <- centimetros/2.54;
    kilogramos <- libras*0.45359237;
    Escribir "El Valor de kilogramos: ", kilogramos;
    Escribir "El Valor de pulgadas: ", pulgadas;
FinAlgoritmo
