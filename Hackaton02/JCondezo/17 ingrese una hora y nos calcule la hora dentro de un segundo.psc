//ingrese una hora y nos calcule la hora dentro de un segundo.
Algoritmo sin_titulo
	Escribir  "Ingresa el valor de centimetros:";
    Leer centimetros;
    Escribir "Ingresa el valor de libras:";
    Leer libras;
    pulgadas <- centimetros/2.54;
    kilogramos <- libras*0.45359237;
    Escribir "El Valor de kilogramos: ", kilogramos;
    Escribir "El Valor de pulgadas: ", pulgadas;
FinAlgoritmo
