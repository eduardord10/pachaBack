Proceso Conversor_de_unidades
	cm=0.0;
	pulgadas=0.0;
	lbs=0.0
	kg=0.0;
	
	Escribir "Ingrese una longitud en centímetros:"
    Leer cm
    pulgadas = cm / 2.54
    Escribir cm, " cm equivale a ", pulgadas, " pulgadas."
	
    Escribir "Ingrese un peso en libras:"
    Leer lbs
    kg = lbs / 2.20462
    Escribir lbs, " lbs equivale a ", kg, " kg."
	
FinProceso
